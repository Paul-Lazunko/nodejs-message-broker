{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "function-name": [true, {
      "function-regex": "^\\*?\\[?[a-zA-Z][\\w\\d\\.]*\\]?$"
    }],
    "max-len": [ "error", {
      "code": 120
    }],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-this-assignment": [ true, {"allowed-names": ["self"], "allow-destructuring": true }]
  }
}
